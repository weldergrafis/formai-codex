{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      },
      "enableLiveMetricsFilters": true
    }
  },
  "extensions": {
    "serviceBus": {
      "transportType": "amqpWebSockets",
      "autoCompleteMessages": false,
      "maxConcurrentCalls": 2
    }
  }
}